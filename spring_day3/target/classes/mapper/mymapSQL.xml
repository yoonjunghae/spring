<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mymap">

	<select id="one" resultType="mapDTO" parameterType="String">
		select * from mymap
		where location = #{location}
	</select>
	
	<select id="all" resultType="mapDTO">
		select * from mymap
	</select>
	
</mapper>











