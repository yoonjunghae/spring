package com.multi.spring.file.model.dao;

import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.stereotype.Repository;

import com.multi.spring.movie.model.dto.MovieDTO;

@Repository
public class FileDao {
	
	public int insertMovie(SqlSessionTemplate sqlSession, FileDTO fileDTO) {
		return sqlSession.insert("movieMapper.insertMovie", fileDTO);
	}

}
